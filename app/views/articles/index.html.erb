<h1>Articles</h1>
<ul id="articles">
<% @articles.each do |article| %>
  <li>
    <%= link_to article.title, article_path(article) %>
  </li>
<% end %>
</ul>


<%= render partial: 'top_article' %>


<% if logged_in? %>
<%= link_to "Create Article", new_article_path, class: "new_article" %>
<% end %>